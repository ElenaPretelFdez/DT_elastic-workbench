{
  "services": [
    {
      "name": "elastic-workbench-video-processing",
      "elasticity_strategies": [
        {
          "ES_name": "resource_scaling",
          "cooldown": 5000,
          "endpoints": [
            {
              "target": "/resource_scaling",
              "parameters": [
                {
                  "name": "cpu_cores",
                  "min": 1,
                  "max": 8
                }
              ]
            }
          ]
        },
        {
          "ES_name": "quality_scaling",
          "cooldown": 5000,
          "endpoints": [
            {
              "target": "/quality_scaling",
              "parameters": [
                {
                  "name": "quality",
                  "min": 360,
                  "max": 1080
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}